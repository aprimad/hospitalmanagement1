<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>addPatient</servlet-name>
        <servlet-class>Controllere.addPatient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FixAppointment</servlet-name>
        <servlet-class>FixAppointment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NurseLogin</servlet-name>
        <servlet-class>NurseLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addPatient</servlet-name>
        <url-pattern>/addPatient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FixAppointment</servlet-name>
        <url-pattern>/FixAppointment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NurseLogin</servlet-name>
        <url-pattern>/NurseLogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
